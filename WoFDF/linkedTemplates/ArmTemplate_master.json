{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WoFDF"
		},
		"WOF Linked Service_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'WOF Linked Service'"
		},
		"WOF_DataWareHouse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'WOF_DataWareHouse'"
		},
		"SageRestGetEntity_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://online.sage.co.za/U62537/api/apibase/entity/get"
		},
		"SageRestToken_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://online.sage.co.za/U62537/token"
		},
		"WOF Linked Service_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "inemuri.database.windows.net"
		},
		"WOF Linked Service_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "WOF"
		},
		"WOF Linked Service_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Inumeri_ETL"
		},
		"WOF_DataWareHouse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "inemuri.database.windows.net"
		},
		"WOF_DataWareHouse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "WOF-Warehouse"
		},
		"WOF_DataWareHouse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Inumeri_ETL"
		},
		"LSSage300_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://online.sage.co.za/U62537/api/apibase/entity/get"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://online.sage.co.za/U62537/api/apibase/entity/get"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "WoFDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"WOF Linked Service_password": {
						"value": "[parameters('WOF Linked Service_password')]"
					},
					"WOF_DataWareHouse_password": {
						"value": "[parameters('WOF_DataWareHouse_password')]"
					},
					"LSSage300_properties_typeProperties_url": {
						"value": "[parameters('LSSage300_properties_typeProperties_url')]"
					},
					"SageRestGetEntity_properties_typeProperties_url": {
						"value": "[parameters('SageRestGetEntity_properties_typeProperties_url')]"
					},
					"SageRestToken_properties_typeProperties_url": {
						"value": "[parameters('SageRestToken_properties_typeProperties_url')]"
					},
					"WOF Linked Service_properties_typeProperties_server": {
						"value": "[parameters('WOF Linked Service_properties_typeProperties_server')]"
					},
					"WOF Linked Service_properties_typeProperties_database": {
						"value": "[parameters('WOF Linked Service_properties_typeProperties_database')]"
					},
					"WOF Linked Service_properties_typeProperties_userName": {
						"value": "[parameters('WOF Linked Service_properties_typeProperties_userName')]"
					},
					"WOF_DataWareHouse_properties_typeProperties_server": {
						"value": "[parameters('WOF_DataWareHouse_properties_typeProperties_server')]"
					},
					"WOF_DataWareHouse_properties_typeProperties_database": {
						"value": "[parameters('WOF_DataWareHouse_properties_typeProperties_database')]"
					},
					"WOF_DataWareHouse_properties_typeProperties_userName": {
						"value": "[parameters('WOF_DataWareHouse_properties_typeProperties_userName')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "WoFDF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'WoFDF_ArmTemplate_0')]"
			]
		}
	]
}